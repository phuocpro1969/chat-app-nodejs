{
  "name": "chat-app",
  "version": "1.0.0",
  "description": "chat-app built from Steven",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc-alias && tsc",
    "start": "pnpm run build && node build/index.js",
    "dev": "npx nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "pnpm run typeorm -d ./src/dbs/datasource.db.ts migration:generate",
    "migration:create": "pnpm run typeorm migration:create",
    "migration:run": "pnpm run typeorm migration:run -d ./src/dbs/datasource.db.ts --",
    "migration:revert": "pnpm run typeorm migration:revert -d ./src/dbs/datasource.db.ts --"
  },
  "author": "Steven",
  "license": "ISC",
  "dependencies": {
    "@socket.io/admin-ui": "^0.5.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.4.0",
    "firebase-admin": "^11.10.1",
    "helmet": "^7.0.0",
    "joi": "^17.10.1",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.85",
    "@swc/helpers": "^0.5.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}